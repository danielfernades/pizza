<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>{% block title %}{% endblock title %}</title>
    {% block stylesheets %}
        <link href="/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
        <link href="/css/bootstrap-responsive.min.css" rel="stylesheet" type="text/css" />
    {% endblock stylesheets %}
    {% block javascripts %}
        <script src="/js/jquery-1.7.2.min.js"></script>
        <script src="/js/bootstrap.min.js"></script>
    {% endblock javascripts %}
</head>
<body>
<div class="container">
    <div class="row">
        <div class="span12">
            {{ "pizza"|trans({}, "frontend") }}
        </div>
    </div>
    <div class="row">
        <div class="span3">
            {% block nav %}
                <ul>
                    <li><a href="{{ path('order_list') }}">{{ "orders"|trans({}, "frontend") }}</a></li>
                    <li><a href="{{ path('user_list') }}">{{ "users"|trans({}, "frontend") }}</a></li>
                    <li><a href="{{ path('logout') }}">{{ "logout"|trans({}, "frontend") }}</a></li>
                </ul>
            {% endblock nav %}
        </div>
        <div class="span9">
            {% block content %}{% endblock content %}
        </div>
    </div>
</div>
</body>
</html>